{
	"name": "@codespartan/lib-ts-core",
	"version": "0.3.0",
	"description": "Librería que contiene las configuraciones básicas para proyectos de Typescript",
	"keywords": [],
	"license": "ISC",
	"author": "",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/core.es.js",
			"require": "./dist/core.cjs.js",
			"types": "./dist/types/core.d.ts"
		}
	},
	"main": "dist/core.cjs.js",
	"module": "dist/core.es.js",
	"types": "dist/types/core.d.ts",
	"files": [
		"dist",
		"package.json",
		"release.js",
		"eslint.config.mjs",
		"commitlint.config.mjs",
		".prettierrc.json",
		".lintstagedrc.mjs",
		".versionrc",
		".czrc"
	],
	"scripts": {
		"build": "vite build && tsc && rollup -c rollup.config.mjs",
		"buildAndChangelog": "npm run build && npm run changelog",
		"changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
		"check": "prettier . --check",
		"check:fix": "prettier . --write",
		"clean": "rimraf ./dist",
		"commit": "cz",
		"dev": "vite",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"preview": "vite preview",
		"release": "standard-version",
		"release:major": "standard-version --release-as major",
		"release:minor": "standard-version --release-as minor",
		"release:patch": "standard-version --release-as patch",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"@commitlint/cli": "19.5.0",
		"@commitlint/config-conventional": "19.5.0",
